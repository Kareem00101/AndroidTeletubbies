<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".UI.MainActivity2">

    <TextView
        android:id="@+id/textView_signIn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Already have an account?"
        android:textColor="@color/cardview_light_background"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.846" />

    <Button
        android:id="@+id/Button_SignUP"
        android:layout_width="289dp"
        android:layout_height="47dp"
        android:layout_marginBottom="55dp"
        android:background="#aa1d1d"
        android:clickable="true"
        android:ems="10"
        android:gravity="center"
        android:inputType="textEmailAddress"
        android:text="  SIGN UP"
        android:textColor="@color/cardview_light_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.788" />

    <EditText
        android:id="@+id/editTextTextEmailAddress6"
        android:layout_width="337dp"
        android:layout_height="53dp"
        android:layout_marginBottom="55dp"
        android:background="@drawable/background"
        android:ems="10"
        android:inputType="textVisiblePassword"
        android:paddingStart="12dp"
        android:hint="Confirm Password"
        android:textColorHint="#b3b3b3"
        android:textColor="#b3b3b3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.659" />

    <EditText
        android:id="@+id/editTextTextEmailAddress5"
        android:layout_width="337dp"
        android:layout_height="53dp"
        android:layout_marginBottom="55dp"
        android:background="@drawable/background"
        android:ems="10"
        android:inputType="textVisiblePassword"
        android:paddingStart="12dp"
        android:hint="Enter Password"
        android:textColorHint="#b3b3b3"
        android:textColor="#b3b3b3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.523" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="337dp"
        android:layout_height="53dp"
        android:layout_marginBottom="55dp"
        android:background="@drawable/background"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:paddingStart="12dp"
        android:hint="Enter your email"
        android:textColorHint="#b3b3b3"
        android:textColor="#b3b3b3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.394" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="140dp"
        android:layout_height="108dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.177"
        app:srcCompat="@drawable/popcorn_33" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="401dp"
        android:layout_height="39dp"
        android:layout_marginStart="140dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="77dp"
        android:gravity="center"
        android:text="Movie Time"
        android:textColor="#c0c0c0"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.116" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="47dp"
        android:layout_height="26dp"
        android:layout_marginStart="177dp"
        android:layout_marginEnd="177dp"
        android:text="-OR-"
        android:textColor="#aa1d1d"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.788" />

    <TextView
        android:id="@+id/textView_signIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign In"
        android:textColor="@color/cardview_light_background"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.658"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.845" />


</androidx.constraintlayout.widget.ConstraintLayout>